<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>Pro Cam 360</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleIconName</key>
	<string>AppIcon</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchScreen</key>
	<dict/>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>NSCameraUsageDescription</key>
	<string>ProCam360 needs camera access to capture photos and videos for your construction and property documentation projects.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>ProCam360 needs microphone access to record voice notes that can be attached to your photos for detailed documentation.</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>ProCam360 uses speech recognition to transcribe your voice notes into text, making them searchable and easier to review.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>ProCam360 uses your location to automatically tag photos with GPS coordinates, helping you organize and locate documentation by property address.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>ProCam360 needs photo library access to save and export your project photos.</string>
</dict>
</plist>
